            compatible = "zmk,behavior-mod-morph";
            label = "Z shortcut layer GUI+Grave";
            bindings = <&ltl 11 Z>, <&kp GRAVE>;

            #binding-cells = <0>;
            mods = <(MOD_LGUI|MOD_RGUI)>;
            keep-mods = <(MOD_LGUI|MOD_RGUI)>;
        };

        dqt_sqt: double_then_single_quote {
            compatible = "zmk,behavior-mod-morph";
