    behaviors {
        hl: hrmod_left {
            compatible = "zmk,behavior-hold-tap";
            label = "Home row mod - Left";
            bindings = <&kp>, <&kp>;

            #binding-cells = <2>;
            quick-tap-ms = <200>;
            require-prior-idle-ms = <70>;
            flavor = "balanced";
            hold-trigger-on-release;
            tapping-term-ms = <240>;
            hold-trigger-key-positions = <3 4 5 12 13 14 15 16 17 24 25 26 27 28 29 35 36 37 38 39 41 46 47 45>;
        };

        hr: hrmod_right {
            compatible = "zmk,behavior-hold-tap";
            label = "Home row mod - Right";
            bindings = <&kp>, <&kp>;

            #binding-cells = <2>;
            quick-tap-ms = <180>;
            require-prior-idle-ms = <150>;
            flavor = "balanced";
            hold-trigger-on-release;
            tapping-term-ms = <240>;
            hold-trigger-key-positions = <0 1 2 6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 40 42 43 44>;
        };

        ltr: layer_tap_right {
            compatible = "zmk,behavior-hold-tap";
            label = "Layer tap - Right";
            bindings = <&mo>, <&kp>;

            #binding-cells = <2>;
            quick-tap-ms = <170>;
            require-prior-idle-ms = <150>;
            flavor = "balanced";
            tapping-term-ms = <170>;
            hold-trigger-key-positions = <30 31 32 33 34>;
        };

        ltl: layer_tap_left {
            compatible = "zmk,behavior-hold-tap";
            label = "Layer tap - Left";
            bindings = <&mo>, <&kp>;

            #binding-cells = <2>;
            quick-tap-ms = <170>;
            require-prior-idle-ms = <150>;
            flavor = "balanced";
            tapping-term-ms = <170>;
            hold-trigger-key-positions = <36 37 38 27 26 25 13 14 15>;
        };

        tmb: miryoku_thumb_layer {
            compatible = "zmk,behavior-hold-tap";
            label = "Thumb layer tap";
            bindings = <&mo>, <&kp>;

            #binding-cells = <2>;
            quick-tap-ms = <240>;
            flavor = "balanced";
            tapping-term-ms = <170>;
            retro-tap;
        };

        tmb_hold_preferred: thumb_layer_hold_preferred {
            compatible = "zmk,behavior-hold-tap";
            label = "Thumb layer tap - hold pref.";
            bindings = <&mo>, <&kp>;

            #binding-cells = <2>;
            quick-tap-ms = <240>;
